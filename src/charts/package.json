{
  "name": "maison-components-charts",
  "version": "1.4.0",
  "private": false,
  "description": "Maison components chart components",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bendigiorgio/maison-components.git"
  },
  "homepage": "https://bendigiorgio.github.io/maison-components",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*/index.js",
      "require": "./dist/*/index.cjs",
      "types": "./dist/*/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "area-chart": [
        "dist/area-chart/"
      ],
      "bar-chart": [
        "dist/bar-chart/"
      ],
      "common": [
        "dist/common/"
      ],
      "donut-chart": [
        "dist/donut-chart/"
      ],
      "line-chart": [
        "dist/line-chart/"
      ],
      "radar-chart": [
        "dist/radar-chart/"
      ]
    }
  },
  "scripts": {
    "build": "NODE_OPTIONS='--max-old-space-size=8192' tsup",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "test": "jest"
  },
  "keywords": [
    "react",
    "typescript",
    "components",
    "next",
    "ui",
    "tailwind"
  ],
  "author": "Ben Di Giorgio",
  "license": "MIT",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "peerDependencies": {
    "react": ">= 17",
    "tailwindcss": ">= 3.3.3"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.1",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "lucide-react": "^0.248.0",
    "maison-components-core": "workspace:*",
    "maison-components-utils": "workspace:*",
    "next-themes": "^0.2.1",
    "react-day-picker": "^8.8.0",
    "react-hook-form": "^7.44.3",
    "recast": "^0.23.2",
    "recharts": "^2.7.2",
    "tailwindcss-animate": "^1.0.6",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "20.3.1",
    "@types/react": "^18.2.12",
    "@types/react-dom": "18.2.5",
    "@types/recharts": "^1.8.24",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "autoprefixer": "^10.4.14",
    "esbuild": "^0.18.6",
    "eslint": "^8.43.0",
    "next": "13.4.6",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "3.3.2",
    "tailwindcss-animate": "^1.0.6",
    "ts-node": "^10.9.1",
    "tsup": "^7.1.0",
    "typescript": "^5.1.3"
  }
}
